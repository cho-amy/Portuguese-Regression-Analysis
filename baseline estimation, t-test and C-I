baseline2.x <- lm(G3.x ~ Medu, data = proj.data) 
baseline2.y <- lm(G3.y ~ Medu, data = proj.data)


# T-tests and confidence intervals

confint(baseline.x, level = 0.95) 
## 2.5 % 97.5 % 
## (Intercept) 10.4627578 11.869887 
## studytime.x 0.4871031 1.121637
confint(baseline.y, level = 0.95) 
## 2.5 % 97.5 % ## (Intercept) 9.837495961 11.6682082 ## studytime.y 0.001509228 0.8290376 

b1.x = coef(baseline.x)[2] 
b1.y = coef(baseline.y)[2] 
se_b1.x = summary(baseline.x)$coef[2,2] 
se_b1.y = summary(baseline.y)$coef[2,2] 
t_b1.x = b1.x/se_b1.x 
t_b1.y = b1.y/se_b1.y 
p_b1.x = 2*(1 - pt(q = t_b1.x, df = nrow(proj.data)-2)) p_b1.x 
## studytime.x  
## 9.824568e-07 
p_b1.y = 2*(1 - pt(q = t_b1.y, df = nrow(proj.data)-2)) p_b1.y 
## studytime.y  
## 0.04917487 




